import java.util.Scanner;

public class ClassMath {
	public static void main(String[] args) {
		double a,b;
		Scanner sc= new Scanner(System.in);
		System.out.print("Nhập a = ");
		a=sc.nextDouble();
		System.out.print("Nhập b = ");
		b=sc.nextDouble();
		
		// Hàm trị tuyệt đối
		System.out.println("|a| = " +ClassMath.abs(a));
		
	}
}

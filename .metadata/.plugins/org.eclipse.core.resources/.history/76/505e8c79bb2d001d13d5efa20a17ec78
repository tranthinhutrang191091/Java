
public class Main {
	public static void main(String[] args) {
		HuynDai car1= new Accent(2.5, 1.5, 500.0, 4, "red", 2000000,"gasoline" );
		System.out.println(car1);
		System.out.println("Giá của Accent sau thuế: "+car1.getSpeedAfterTax()+"VNĐ");
		car1.startUp();
		car1.acceleration();
		car1.stop();
		
		
	}
}

package objectcollection;

import java.util.Set;
import java.util.TreeMap;

import javax.security.auth.Subject;

import object.Student;

public class StudentSubject {
	private Student student;
	private TreeMap<Subject, Integer> mapSubjcet;
	private TreeMap<Student, TreeMap> mapStudentSubjcet;
	public StudentSubject() {
		this.student= new Student();
		this.mapSubjcet= new TreeMap<Subject,Integer>();
		this.mapStudentSubjcet= new TreeMap<Student, TreeMap>();
	}
	public StudentSubject(Student student, TreeMap<Subject, Integer> mapSubjcet) {
		this.student = student;
		this.mapSubjcet = mapSubjcet;
		this.mapStudentSubjcet.put(student,mapSubjcet);
	}
	public Student getStudent() {
		return student;
	}
	public void setStudent(Student student) {
		this.student = student;
	}
	public TreeMap<Subject, Integer> getMapSubjcet() {
		return mapSubjcet;
	}
	public void setMapSubjcet(TreeMap<Subject, Integer> mapSubjcet) {
		this.mapSubjcet = mapSubjcet;
	}
	
	public TreeMap<Student, TreeMap> getMapStudentSubjcet() {
		return mapStudentSubjcet;
	}
	public void setMapStudentSubjcet(TreeMap<Student, TreeMap> mapStudentSubjcet) {
		this.mapStudentSubjcet = mapStudentSubjcet;
	}
	@Override
	public String toString() {
		String result="";
		Set<Student> setStudent= this.mapStudentSubjcet.keySet();
		for (Student key : setStudent) {
			result+=key + "|" + this.mapStudentSubjcet.get(key);
		}
		return "|" + student + ", mapSubjcet=" + mapSubjcet + "]";
	}
	
	
}

package overloadinganimal;
public class Bird extends Animal {
	public Bird() {
		super("Bird");
	}

	public void fly() {
		System.out.println("Fly");
	}

	@Override
	public void eat() {
		System.out.println("Tôi ăn sâu!");
	}

	@Override
	public void makeSound() {
		System.out.println("chíp chíp!");
	}
}

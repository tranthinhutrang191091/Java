package objectcollection;

import java.util.HashMap;
import java.util.HashSet;
import java.util.LinkedList;

import object.Subject;

public class SubjectScore {
	private Subject setSubject;
	private Float listScore;
	private HashMap<Subject, Float> mapSubjcetScore= new HashMap<Subject, Float>();
	public SubjectScore() {
	}
	public SubjectScore(Subject setSubject, Float listScore) {
		this.setSubject = setSubject;
		this.listScore = listScore;
		this.mapSubjcetScore.put(setSubject, listScore);
	}
	
}

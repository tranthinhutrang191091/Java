package object;

import java.util.LinkedList;
import java.util.Map;
import java.util.TreeMap;

public class ClassStudent {
	private Class classO;
	private LinkedList <Student> listStudent;
	private TreeMap<Class, LinkedList<Student>> mapClass;
	
	public ClassStudent() {
		this.mapClass = new TreeMap<Class, LinkedList<Student>>;
	}

	public ClassStudent(Class classO, LinkedList<Student> listStudent, TreeMap<Class, LinkedList<Student>> mapClass) {
		this.classO = classO;
		this.listStudent = listStudent;
		this.mapClass = mapClass;
	}
	
	
}

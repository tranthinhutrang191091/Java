
public class Main {
	public static void main(String[] args) {
		Car car1= new Accent(2.5, 1.5, 500.0, 4, "red", 1000000000,"gasoline" );
		HuynDai car2 = new Accent(3.5, 2.0, 600.0, 7, "blue", 150000000,"oil" );
		System.out.println(car1);
		System.out.println("Price of Accent after tax: "+car1.getSpeedAfterTax()+"VNĐ");
		car1.startUp();
		car1.acceleration();
		car1.stop();
		
		Car car3= new SantaFe(2.5, 1.5, 500.0, 4, "red", 2000000,"gasoline" );
		System.out.println(car2);
		System.out.println("Price of SantaFe after tax: "+car2.getSpeedAfterTax()+"VNĐ");
		car2.startUp();
		car2.acceleration();
		car2.stop();
		
		
		
	}
}

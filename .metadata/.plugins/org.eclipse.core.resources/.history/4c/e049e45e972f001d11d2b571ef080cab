package com.daicent;

import java.util.Random;
// Lấy giá trị ở Thread Four sang
public class ThreadFive extends Thread {
	
	@Override
	public void run() {
		Random random= new Random();
		for(int i=0;i<10;i++) {
			int rad = random.nextInt(100);
			System.out.println("Thread Four => "+rad);
		}
	}
	
}

package ObjectCollection;

import java.util.HashSet;
import java.util.Set;

import Object.School;
import object.Student;

public class ListSchool {
		private HashSet<School> listSchool;

		public ListSchool(HashSet<School> listSchool) {
			this.listSchool = listSchool;
		}

		public HashSet<School> getListSchool() {
			return listSchool;
		}

		public void setListSchool(HashSet<School> listSchool) {
			this.listSchool = listSchool;
		}
		private void loadListSchool() {
			for(School school : listSchool) {
				System.out.println(school);
			}
		}
		
		
}

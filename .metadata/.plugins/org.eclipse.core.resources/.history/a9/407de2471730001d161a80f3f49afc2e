package objectcollection;

import java.util.HashSet;
import java.util.LinkedList;
import java.util.Set;
import java.util.TreeMap;

import object.Student;
import object.Subject;

public class StudentSubject {
	private Student student;
	private HashSet<Subject> setSubject;
	private LinkedList<Float> listScore;
	private TreeMap<HashSet, LinkedList> treeScore= new TreeMap<HashSet, LinkedList>();
	private TreeMap<Student,TreeMap> mapStudent = new TreeMap<Student, TreeMap>();
	
	public StudentSubject() {
		this.student = new Student();
		this.treeScore = new TreeMap<HashSet, LinkedList>();
		this.mapStudent = new TreeMap<Student, TreeMap>();
	}

	public StudentSubject(Student student,TreeMap<Subject, Float> treeScore) {
		this.student = student;
		this.mapStudent.put(student, treeScore);
		
	}

	public Student getStudent() {
		return student;
	}

	public void setStudent(Student student) {
		this.student = student;
	}

	@Override
	public String toString() {
		String resuft = "";
		Set<Student> setStudent = mapStudent.keySet();
		for (Student key : setStudent) {
			System.out.println(key + "\t" + mapStudent.get(key));
		}
		return resuft;
	}
	
}
